{
  "manifest_version": 3,
  "name": "Change Product's Title Color",
  "version": "1.0",
  "description": "Changing the product's title color when on the product page",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.fashionnova.com/products/*"
      ],
      "js": [
        "jquery.min.js",
        "content.js"
      ]
    }
  ],
  "action": {
    "default_icon": "images/icon.png"
  }
}
